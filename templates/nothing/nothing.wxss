.nothing {
    position: fixed;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    text-align: center;
}

.nothing-img {
    width: 174rpx;
    height: 200rpx;
    margin-bottom: 16rpx;
}

.nothing-text {
    font-size: 32rpx;
    color: #999;
}